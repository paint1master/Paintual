<Window x:Class="PaintualUI.Controls.MessageWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PaintualUI.Controls"
        mc:Ignorable="d"
        Title="MessageWindow" Height="300" Width="300">
    <Grid>
        <Canvas Background="#FF505050" Margin="0,0,0,0">
            <Label Name="lblTitle" Margin="10,10" Width="200" Height="24" Content="Title" />
            <Label Name="lblMessage" Width="260" Height="190" Content="Message" Canvas.Left="10" Canvas.Top="30" />
            <Button Name="btnClose" Height="26" Width="86" Canvas.Left="196" Canvas.Top="233" Content="Close" Click="btnClose_Click" />
        </Canvas>
    </Grid>
</Window>
